{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "es2019"],
    "moduleResolution": "node",
    "module": "esnext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "typeRoots": ["./node_modules/@types/", "./typings/", "./node_modules/@ragempcommunity"],
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    }
  }
}
